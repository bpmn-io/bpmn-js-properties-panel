    "appliesTo": ["bpmn:Task"],
    "properties": [
      {
        "label": "resultStatus",
        "value": "resultStatus",
        "binding": {
          "type": "camunda:outputParameter",
          "source": "${resultStatus}"
        }
      },
      {
