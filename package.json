    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.2.2",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^17.0.0",
    "raw-loader": "^4.0.2",
    "react-svg-loader": "^3.0.3",
    "rollup": "^2.52.6",
    "rollup-plugin-copy": "^3.4.0",
