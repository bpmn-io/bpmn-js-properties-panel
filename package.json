    "karma-webpack": "^5.0.1",
    "mocha": "^10.7.3",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all2": "^8.0.0",
    "puppeteer": "^24.16.2",
    "raw-loader": "^4.0.2",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-react-svg": "^3.0.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.101.2",
    "zeebe-bpmn-moddle": "^1.11.0"
